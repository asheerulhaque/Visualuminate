{
  "version": 2,
  "builds": [
    {
      "src": "server/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/all-data", "dest": "server/index.js" },
    { "src": "/api/(.*)", "dest": "server/index.js" },
    { "src": "/(.*)", "dest": "server/index.js" }
  ]
}
